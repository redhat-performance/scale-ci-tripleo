{# Determine the OSP Series name via a dictionary on $version #}
{% set osp_series = {"10": "newton", "11": "ocata", "12": "pike", "13": "queens"}[version] | default("Unknown Version") -%}
{
    "osp_series": "{{osp_series}}",
    "osp_version": "{{version}}",
    "rhos_release": "{{rhos_release}}",
    "build": "{{build}}",
    "uc_build_date": "{{lookup('pipe','date +%Y%m%d-%H%M%S')}}"
}
